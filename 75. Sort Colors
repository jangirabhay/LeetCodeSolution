class Solution {
    public void sortColors(int[] nums) {
        ArrayList<Integer> list = new ArrayList<>();
        for(int i=0; i<nums.length; i++){
            list.add(nums[i]);
        }
        Collections.sort(list);
        int j=0;
        for(int i=0; i<list.size(); i++){
           nums[j] =  list.get(i);
           j++;
        }
        
    }
}
